<div class="highlight highlight-source-js-jsx"><pre><span class="pl-k">import</span> <span class="pl-smi">AwesomeSlider</span> <span class="pl-k">from</span> <span class="pl-s"><span class="pl-pds">'</span>react-awesome-slider<span class="pl-pds">'</span></span>;
<span class="pl-k">import</span> <span class="pl-smi">AwesomeSliderStyles</span> <span class="pl-k">from</span> <span class="pl-s"><span class="pl-pds">'</span>react-awesome-slider/src/styles<span class="pl-pds">'</span></span>;

<span class="pl-k">const</span> <span class="pl-smi">slider</span> <span class="pl-k">=</span> (
  &lt;<span class="pl-ent">AwesomeSlider</span> <span class="pl-e">cssModule</span><span class="pl-k">=</span><span class="pl-pse">{</span><span class="pl-smi">AwesomeSliderStyles</span><span class="pl-pse">}</span>&gt;
    &lt;<span class="pl-ent">div</span> <span class="pl-e">data-src</span><span class="pl-k">=</span><span class="pl-s"><span class="pl-pds">"</span>/path/to/image-0.png<span class="pl-pds">"</span></span> /&gt;
    &lt;<span class="pl-ent">div</span> <span class="pl-e">data-src</span><span class="pl-k">=</span><span class="pl-s"><span class="pl-pds">"</span>/path/to/image-1.png<span class="pl-pds">"</span></span> /&gt;
    &lt;<span class="pl-ent">div</span> <span class="pl-e">data-src</span><span class="pl-k">=</span><span class="pl-s"><span class="pl-pds">"</span>/path/to/image-2.jpg<span class="pl-pds">"</span></span> /&gt;
  &lt;/<span class="pl-ent">AwesomeSlider</span>&gt;
);</pre></div>
<h3><a id="user-content-fold-out-animation-recipe-with-css-modules" class="anchor" aria-hidden="true" href="#fold-out-animation-recipe-with-css-modules"><svg class="octicon octicon-link" viewBox="0 0 16 16" version="1.1" width="16" height="16" aria-hidden="true"><path fill-rule="evenodd" clip-rule="evenodd" d="M7.775 3.275C7.64252 3.41717 7.57039 3.60522 7.57382 3.79952C7.57725 3.99382 7.65596 4.1792 7.79337 4.31662C7.93079 4.45403 8.11617 4.53274 8.31047 4.53617C8.50477 4.5396 8.69282 4.46748 8.835 4.335L10.085 3.085C10.2708 2.89918 10.4914 2.75177 10.7342 2.65121C10.977 2.55064 11.2372 2.49888 11.5 2.49888C11.7628 2.49888 12.023 2.55064 12.2658 2.65121C12.5086 2.75177 12.7292 2.89918 12.915 3.085C13.1008 3.27082 13.2482 3.49142 13.3488 3.7342C13.4493 3.97699 13.5011 4.23721 13.5011 4.5C13.5011 4.76279 13.4493 5.023 13.3488 5.26579C13.2482 5.50857 13.1008 5.72917 12.915 5.915L10.415 8.415C10.2292 8.60095 10.0087 8.74847 9.76588 8.84911C9.52308 8.94976 9.26283 9.00157 9 9.00157C8.73716 9.00157 8.47691 8.94976 8.23411 8.84911C7.99132 8.74847 7.77074 8.60095 7.585 8.415C7.44282 8.28252 7.25477 8.21039 7.06047 8.21382C6.86617 8.21725 6.68079 8.29596 6.54337 8.43337C6.40596 8.57079 6.32725 8.75617 6.32382 8.95047C6.32039 9.14477 6.39252 9.33282 6.525 9.475C6.85001 9.80004 7.23586 10.0579 7.66052 10.2338C8.08518 10.4097 8.54034 10.5002 9 10.5002C9.45965 10.5002 9.91481 10.4097 10.3395 10.2338C10.7641 10.0579 11.15 9.80004 11.475 9.475L13.975 6.975C14.6314 6.31858 15.0002 5.4283 15.0002 4.5C15.0002 3.57169 14.6314 2.68141 13.975 2.025C13.3186 1.36858 12.4283 0.999817 11.5 0.999817C10.5717 0.999817 9.68141 1.36858 9.02499 2.025L7.775 3.275ZM3.085 12.915C2.89904 12.7292 2.75152 12.5087 2.65088 12.2659C2.55023 12.0231 2.49842 11.7628 2.49842 11.5C2.49842 11.2372 2.55023 10.9769 2.65088 10.7341C2.75152 10.4913 2.89904 10.2707 3.085 10.085L5.585 7.585C5.77074 7.39904 5.99132 7.25152 6.23411 7.15088C6.47691 7.05023 6.73716 6.99842 7 6.99842C7.26283 6.99842 7.52308 7.05023 7.765