<span class="pl-k">const</span> <span class="pl-smi">component</span> <span class="pl-k">=</span> (
  &lt;<span class="pl-ent">CaptionedSlider</span>
    <span class="pl-e">startupScreen</span><span class="pl-k">=</span><span class="pl-pse">{</span><span class="pl-smi">StartupScreen</span><span class="pl-pse">}</span>
