<span class="pl-k">const</span> <span class="pl-smi">AutoplaySlider</span> <span class="pl-k">=</span> <span class="pl-en">withAutoplay</span>(<span class="pl-smi">AwesomeSlider</span>);

<span class="pl-k">const</span> <span class="pl-smi">slider</span> <span class="pl-k">=</span> (
  &lt;<span class="pl-ent">AutoplaySlider</span>
    <span class="pl-e">play</span><span class="pl-k">=</span><span class="pl-pse">{</span><span class="pl-c1">true</span><span class="pl-pse">}</span>
    <span class="pl-e">cancelOnInteraction</span><span class="pl-k">=</span><span class="pl-pse">{</span><span class="pl-c1">false</span><span class="pl-pse">}</span> <span class="pl-c"><span class="pl-c">//</span> should stop playing on user interaction</span>
    <span class="pl-e">interval</span><span class="pl-k">=</span><span class="pl-pse">{</span><span class="pl-c1">6000</span><span class="pl-pse">}</span>
  &gt;
    &lt;<span class="pl-ent">div</span> <span class="pl-e">data-src</span><span class="pl-k">=</span><span class="pl-s"><span class="pl-pds">"</span>/path/to/image-0.png<span class="pl-pds">"</span></span> /&gt;
    &lt;<span class="pl-ent">div</span> <span class="pl-e">data-src</span><span class="pl-k">=</span><span class="pl-s"><span class="pl-pds">"</span>/path/to/image-1.png<span class="pl-pds">"</span></span> /&gt;
    &lt;<span class="pl-ent">div</span> <span class="pl-e">data-src</span><span class="pl-k">=</span><span class="pl-s"><span class="pl-pds">"</span>/path/to/image-2.jpg<span class="pl-pds">"</span></span> /&gt;
  &lt;/<span class="pl-ent">AutoplaySlider</span>&gt;
);</pre></div>
