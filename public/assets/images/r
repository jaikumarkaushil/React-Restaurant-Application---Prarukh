  &lt;<span class="pl-ent">AwesomeSlider</span>
    <span class="pl-e">media</span><span class="pl-k">=</span><span class="pl-pse">{</span>[
      {
        <span class="pl-c1"><span class="pl-s">source</span>:</span> <span class="pl-s"><span class="pl-pds">'</span>/path/to/image-0.png<span class="pl-pds">'</span></span>,
