    <span class="pl-e">screens</span><span class="pl-k">=</span><span class="pl-pse">{</span>[
      {
        <span class="pl-c1"><span class="pl-s">backgroundColor</span>:</span> <span class="pl-s"><span class="pl-pds">'</span>#4a9c8c<span class="pl-pds">'</span></span>,
        <span class="pl-c1"><span class="pl-s">media</span>:</span> <span class="pl-s"><span class="pl-pds">'</span>/images/series/ricknmorty-3.png<span class="pl-pds">'</span></span>,
        <span class="pl-c1"><span class="pl-s">caption</span>:</span> <span class="pl-s"><span class="pl-pds">'</span>I want to see what you got.<span class="pl-pds">'</span></span>,
      },
      {
        <span class="pl-c1"><span class="pl-s">backgroundColor</span>:</span> <span class="pl-s"><span class="pl-pds">'</span>#4a9c8c<span class="pl-pds">'</span></span>,
